{
	"presets": [
		"next/babel",
		[
			"@babel/preset-env",
			{
				"targets": {
					"node": "current",
					"esmodules": true
				}
			}
		]
	],
	"plugins": [
		[
			"styled-components",
			{"ssr": true, "displayName": true, "preprocess": false}
		],
		"@babel/plugin-proposal-class-properties",
		[
			"@babel/plugin-transform-runtime",
			{
				"regenerator": true
			}
		]
	]
}
